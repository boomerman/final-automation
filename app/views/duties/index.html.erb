<p id="notice"><%= notice %></p>

<h1>Duties</h1>

<table class="table">
  <thead>
    <tr>
      <th>Group</th>
      <th>Panel</th>
      <th>Date</th>
      <th>Time</th>
      <th>Venue</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @duties.each do |duty| %>
      <tr>
        <td><%= duty.group_id %></td>
        <td><%= duty.panel_id %></td>
        <td><%= duty.date %></td>
        <td><%= duty.time %></td>
        <td><%= duty.venue %></td>
        <td><%= link_to 'Show', duty %></td>
        <td><%= link_to 'Edit', edit_duty_path(duty) %></td>
        <td><%= link_to 'Destroy', duty, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Duty', new_duty_path %>
<%= link_to 'Back', coordinator_path %>