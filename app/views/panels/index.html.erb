<p id="notice"><%= notice %></p>

<h1>Panels</h1>

<table class="table">
  <thead>
    <tr>
      <th>Panel</th>
      <th>Teacher 1</th>
      <th>Teacher 2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @panels.each do |panel| %>
      <tr>
        <td><%= panel.panel_id %></td>
        <td><%= panel.teacher_1 %></td>
        <td><%= panel.teacher_2 %></td>
        <td><%= link_to 'Show', panel %></td>
        <td><%= link_to 'Edit', edit_panel_path(panel) %></td>
        <td><%= link_to 'Destroy', panel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Panel', new_panel_path %>
<%= link_to 'Go back', coordinator_path %>
